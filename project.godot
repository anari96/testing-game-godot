config_version=5

[application]

config/name="dungeoun_crawler"
run/main_scene="res://scene/map/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.304822, 0.28644, 0.581366, 1)
config/icon="res://icon.svg"

[autoload]

SceneManager="*res://script/autoload/scene_manager.gd"
SpawnManager="*res://script/autoload/spawn_manager.gd"
InventoryManager="*res://script/autoload/inventory_manager.gd"
PlayerManager="*res://script/autoload/player_manager.gd"
DialogueManager="*res://script/autoload/dialogue_manager.gd"
FactManager="*res://script/autoload/fact_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false
window/vsync/vsync_mode=0

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

